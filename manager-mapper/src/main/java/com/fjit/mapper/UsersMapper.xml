<?xml version="1.0" encoding="UTF-8" ?>
        <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
                "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fjit.mapper.UsersMapper">
<insert id="insertUser" parameterType="com.fjit.pojo.Users">
        insert into user(uname,pwd) values(#{uname},#{pwd})
    </insert>

<select id="selectUserAll" resultType="com.fjit.pojo.Users">
        select * from user
    </select>
</mapper>